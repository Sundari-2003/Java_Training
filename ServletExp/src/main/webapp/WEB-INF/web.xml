<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>MyFilter</filter-name>
  	<display-name>MyFilter</display-name>
  	<description></description>
  	<filter-class>com.sundari.ServletDemo.MyFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>MyFilter</filter-name>
  	<url-pattern>/MyFilter</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>com.sundari.ServletExp.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HelloServlet1</servlet-name>
  	<display-name>HelloServlet1</display-name>
  	<description></description>
  	<servlet-class>com.sundari.ServletDemo.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DemoSevlet</servlet-name>
  	<display-name>DemoSevlet</display-name>
  	<description></description>
  	<servlet-class>com.sundari.ServletDemo.DemoSevlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HelloServlet1</servlet-name>
  	<url-pattern>/HelloServlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DemoSevlet</servlet-name>
  	<url-pattern>/DemoSevlet</url-pattern>
  </servlet-mapping>
</web-app>
