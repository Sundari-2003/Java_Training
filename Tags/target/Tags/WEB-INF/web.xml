<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SyncServlet</servlet-name>
  	<display-name>SyncServlet</display-name>
  	<description></description>
  	<servlet-class>com.thanu.SyncServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AsynServlet</servlet-name>
  	<display-name>AsynServlet</display-name>
  	<description></description>
  	<servlet-class>com.thanu.AsynServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>SyncServlet</servlet-name>
  	<url-pattern>/sync</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AsynServlet</servlet-name>
  	<url-pattern>/async</url-pattern>
  </servlet-mapping>
</web-app>
